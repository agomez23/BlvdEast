name = "Boulevard East"
description = Boulevard East theme created by <a href="http://www.onlinemarketinggrp.com/">Online Marketing Group</a>.
base theme = fusion_core
core = 6.x
engine = phptemplate

stylesheets[all][]	= css/reset.css
stylesheets[all][]	= css/typography.css
stylesheets[all][]	= css/general.css
stylesheets[all][]	= css/listing.css
stylesheets[all][]	= css/blvd.css
stylesheets[all][]	= css/css3.css
;conditional-stylesheets[if IE][all][] = css/ie.css

scripts[] = js/modernizr-1.1.min.js
scripts[] = js/script.js

regions[header_bar_middle] = header bar middle
regions[header_bar] = header bar
regions[tools] = tools
regions[sidebar_first] = sidebar first
regions[sidebar_last] = sidebar last
regions[content] = content
regions[footer] = footer

features[] = logo
features[] = name
features[] = slogan
features[] = node_user_picture
features[] = comment_user_picture
features[] = search
features[] = favicon
features[] = primary_links
features[] = secondary_links

; Define panels layout directory.
plugins[panels][layouts] = plugins/layouts

; Default theme settings
settings[theme_grid] = grid16-960
settings[theme_grid_options][] = grid16-960
settings[theme_grid_options][] = grid16-fluid
settings[theme_grid_options][] = grid12-960
settings[theme_grid_options][] = grid12-fluid
settings[fluid_grid_width] = fluid-100
settings[theme_font_size] = font-size-12
settings[sidebar_layout] = sidebars-split
settings[sidebar_first_width] = 2
settings[sidebar_last_width] = 2

; Inherit Skinr styles from Fusion Core
skinr[options][inherit_skins] = false

; Widths for 16 column, 960px/fluid grid
skinr[grid16-width][title] = Width (16 column grid)
skinr[grid16-width][type] = select
